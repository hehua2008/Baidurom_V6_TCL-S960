    <header android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.jrdsettings.DevelopmentSettings" />
    <header android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.android.jrdsettings.DeviceInfoSettings" />
    <header android:icon="@drawable/ic_settings_quick_boot" android:id="@id/quick_boot_settings" android:title="@string/quick_boot_settings_title" android:summary="@string/quick_boot_settings_summary" />
